<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  jcr:primaryType="nt:unstructured"
  sling:resourceType="kestros/cms/dialog"
  jcr:title="Edit Component">
  <content-area jcr:primaryType="nt:unstructured">
    <tabs
      jcr:primaryType="nt:unstructured"
      sling:resourceType="kestros/cms/general/tabs"
      container="true">
      <general jcr:primaryType="nt:unstructured"
        sling:resourceType="kestros/cms/general/container"
        jcr:title="General"
        container="true">
        <allow-components-before
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kestros/cms/fields/checkbox"
          description=""
          propertyName="allowComponentsBefore"
          label="Allow Components Before"
          required="false"
          autoFocus="false"
          initialValue="false"
          fieldValidators=""
          defaultValue=""/>
        <allow-components-after
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kestros/cms/fields/checkbox"
          description=""
          propertyName="allowComponentsAfter"
          label="Allow Components After"
          required="false"
          autoFocus="false"
          initialValue="false"
          fieldValidators=""
          defaultValue=""/>
      </general>
    </tabs>
  </content-area>
</jcr:root>
